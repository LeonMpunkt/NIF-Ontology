        <obo_annot:createdDate rdf:datatype="&xsd;string">May 21, 2010</obo_annot:createdDate>
        <obo_annot:synonym rdf:datatype="&xsd;string">Neuropharmacology</obo_annot:synonym>
        <obo_annot:synonym rdf:datatype="&xsd;string">Psychopharmacology</obo_annot:synonym>
        <core:definition rdf:datatype="&xsd;string">is concerned with drug-induced changes in the functioning of cells.</core:definition>
    </owl:Class>
    


    <!-- http://ontology.neuinfo.org/NIF/DigitalEntities/NIF-Scientific-Discipline.owl#nlx_inv_100664 -->

    <owl:Class rdf:about="&nif_sci_disc;nlx_inv_100664">
        <rdfs:label rdf:datatype="&xsd;string">Philosophy</rdfs:label>
        <rdfs:subClassOf rdf:resource="&nif_sci_disc;nlx_inv_100601"/>
        <obo_annot:synonym rdf:datatype="&xsd;string"></obo_annot:synonym>
        <obo_annot:abbrev rdf:datatype="&xsd;string"></obo_annot:abbrev>
        <dc:contributor rdf:datatype="&xsd;string">Anita Bandrowski</dc:contributor>
        <obo_annot:createdDate rdf:datatype="&xsd;string">May 21, 2010</obo_annot:createdDate>
        <core:definition rdf:datatype="&xsd;string">is the study of general and fundamental problems concerning matters such as existence, knowledge, truth, beauty, law, justice, validity, mind, and language. Philosophy is distinguished from other ways of addressing these questions (such as mysticism or mythology) by its critical, generally systematic approach and its reliance on reasoned argument. The word is of Greek origin: φιλοσοφία, philosophía, &quot;love of wisdom&quot;. - adapted from Wikipedia</core:definition>
    </owl:Class>
    


    <!-- http://ontology.neuinfo.org/NIF/DigitalEntities/NIF-Scientific-Discipline.owl#nlx_inv_100665 -->

    <owl:Class rdf:about="&nif_sci_disc;nlx_inv_100665">
