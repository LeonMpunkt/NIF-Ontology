BIRNLEX:5 a owl:Class ;
    owl:deprecated true ;
    rdfs:label "Biomaterial object"^^xsd:string ;
    skos:prefLabel "Biomaterial object"^^xsd:string ;
    rdfs:subClassOf NIFRID:birnlexRetiredClass ;
