:birnlex_1803 a owl:Class ;
    rdfs:label "Touch sensation"^^xsd:string ;
    core:prefLabel "Touch sensation"^^xsd:string ;
    obo_annot:synonym "Tactile perception"^^xsd:string,
        "Tactile sensation"^^xsd:string,
        "Touch perception"^^xsd:string ;
    rdfs:subClassOf :birnlex_1822 ;
    owl:disjointWith :birnlex_1806,
        :birnlex_1814,
        :birnlex_1828,
