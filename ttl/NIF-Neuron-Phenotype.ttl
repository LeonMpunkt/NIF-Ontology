<http://ontology.neuinfo.org/NIF/ttl/NIF-Neuron-Phenotype.ttl> a owl:Ontology ;
    rdfs:label "NIF Neuron phenotypes" ;
    rdfs:comment "The NIF Neuron phenotype ontology holds neuron phenotypes." .

### Object Properties

ilx:hasAxonLocatedIn a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasAxonLocatedIn" ;
    rdfs:subPropertyOf ilx:hasLocationPhenotype .

ilx:hasDendriteLocatedIn a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasDendriteLocatedIn" ;
    rdfs:subPropertyOf ilx:hasLocationPhenotype .

ilx:hasDevelopmentalOrigin a owl:ObjectProperty ;
    rdfs:label "hasDevelopmentalOrigin" ;
    skos:definition "Expresses a relationship between a neuron type and a developmental cell population. This should be used when a neuron develops from an identified developmental cell population." .

ilx:hasElectrophysiologicalPhenotype a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasElectrophysiologicalPhenotype" ;
    skos:definition "Expresses a relationship between a neuron type and an electrophysiological phenotype concept. This should be used when a neuron type is described using a high level electrophysiological concept class." ;
    rdfs:subPropertyOf ilx:hasIntrinsicPhenotype .

ilx:hasExpressionPhenotype a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasExpressionPhenotype" ;
    skos:definition "Expresses a relationship between a neuron type and a gene type. This should be used for detection of RNAs that code for proteins which may or may not have been detected directly." ;
    rdfs:subPropertyOf ilx:hasIntrinsicPhenotype .

ilx:hasExtrinsicPhenotype a owl:ObjectProperty ;
    rdfs:label "hasExtrinsicPhenotype" ;
    rdfs:subPropertyOf ilx:hasPhenotype .

ilx:hasInstanceInSpecies a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasInstanceInSpecies" ;
    skos:definition "Expresses a relationship between a neuron type and a species. This should be used when a neuron type has been observed in a specific species." ;
    rdfs:subPropertyOf ilx:hasExtrinsicPhenotype .

ilx:hasIntrinsicPhenotype a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasIntrinsicPhenotype" ;
    rdfs:subPropertyOf ilx:hasPhenotype .

ilx:hasLayerLocationPhenotype a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasLayerLocationPhenotype" ;
    skos:definition "Expresses a relationship between a neuron type and a laminer brain region." ;
    rdfs:subPropertyOf ilx:hasLocationPhenotype .

ilx:hasLocationPhenotype a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasLocationPhenotype" ;
    skos:definition "Expresses a relationship between a neuron type and a brain region. This is a superProperty and should not be used directly." ;
    rdfs:subPropertyOf ilx:hasExtrinsicPhenotype .

ilx:hasMolecularPhenotype a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasMolecularPhenotype" ;
    skos:definition "Expresses a relationship between a neuron type and a molecule type. This should be used for direct detection of the molecule, whether it is a small molecule or a peptide." ;
    rdfs:subPropertyOf ilx:hasExpressionPhenotype .

ilx:hasMorphologicalPhenotype a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasMorphologicalPhenotype" ;
    skos:definition "Expresses a relationship between a neuron type and a morphological phenotype concept. This should be used when a neuron type is described using a high level morphological concept class." ;
    rdfs:subPropertyOf ilx:hasIntrinsicPhenotype .

ilx:hasNeurotransmitterPhenotype a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasNeurotransmitterPhenotype" ;
    rdfs:subPropertyOf ilx:hasExpressionPhenotype .

ilx:hasPhenotype a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasPhenotype" ;
    owl:inverseOf ilx:phenotypeOf .

ilx:hasPresynapticTerminalsIn a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasPresynapticTerminalsIn" .

ilx:hasProjectionPhenotype a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasProjectionPhenotype" ;
    skos:definition "Expresses a relationship between a neuron type and a brain region that said neuron type sends axons to." ;
    rdfs:subPropertyOf ilx:hasPhenotype .

ilx:hasSomaLocatedIn a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasSomaLocatedIn" ;
    rdfs:subPropertyOf ilx:hasLocationPhenotype .

ilx:hasSpikingPhenotype a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasSpikingPhenotype" ;
    rdfs:subPropertyOf ilx:hasElectrophysiologicalPhenotype .

ilx:hasTaxonRank a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "hasTaxonRank" ;
    skos:definition "Expresses a relationship between a neuron type and a taxon. This should be used when a neuron type is thought to have been present in the last common ancestor of the taxon." ;
    rdfs:subPropertyOf ilx:hasExtrinsicPhenotype .

ilx:phenotypeCooccuresWith a owl:ObjectProperty ;
    rdfs:label "phenotypeCooccuresWith" ;
    skos:definition "Expresses a relationship between a phenotype and a phenotype. This should be used when two phenotypes commonly cooccure." .

ilx:phenotypeObservedInBrainRegion a owl:ObjectProperty ;
    rdfs:label "phenotypeObservedInBrainRegion" ;
    skos:definition "Expresses a relationship between a phenotype and a brain region. This should be used when a neuron phenotype is observed in specific brain regions." .

ilx:phenotypeOf a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "phenotypeOf" .

### Classes

ilx:AccomodatingSpikingPhenotype a owl:Class ;
    rdfs:label "Accomodating Spiking Phenotype" ;
    rdfs:subClassOf ilx:SpikingPhenotype ;
    owl:disjointWith ilx:NonAccomodatingSpikingPhenotype ;
    ilx:useObjectProperty ilx:hasElectrophysiologicalPhenotype ;
    skos:hiddenLabel "Accomodating Spiking" .

