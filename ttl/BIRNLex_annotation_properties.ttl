    rdfs:label "_limbo_class"@en ;
    core:prefLabel "_birnlex_limbo_class"^^xsd:string ;
    core:definition "class in limbo since major refactoring of foundational community ontologies (e.g., BFO, OBI, SKOS). (BB:2007-03-05)."^^xsd:string ;
    rdfs:subClassOf owl:Thing ;
    obo_annot:createdDate "2007-03-05"^^xsd:string ;
    obo_annot:modifiedDate "2007-03-05"^^xsd:string ;
