          <obo_annot:ExternalSource rdf:ID="MeSH-UMLS"/>
          <obo_annot:ExternalSource rdf:ID="NCBI_Taxonomy"/>
          <obo_annot:ExternalSource rdf:ID="GBIF"/>
          <obo_annot:ExternalSource rdf:ID="OBR"/>
